/* pages/collect/collect.wxss */
.collect_wrap{
    width: 94%;
    margin: 0 auto;
}
.c_box{
    border-bottom: 1px solid #e5e5e5;
}
.name{
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding: 10rpx 0 15rpx;
}
.companyname{
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding-bottom:15rpx;
    font-size: 13pt;
    color: #999999;
}
.c_box .bottom{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-bottom:15rpx;
}
.c_box .bottom .left ,.c_box .bottom .right{
    color: #999999;
    font-size: 13pt;
}
.c_box .bottom .right text{
    color: #e58026;
}