<!--pages/companyDetail/companyDetail.wxml-->
<view class="companyDetail_wrap position_intro">
    <image mode="aspectFit" class="company_image" src="image/555.png"></image>
        <view class="p_name">{{position.company}}</view>
        <view class="p_list_box">
            <ul>
                <li>
                    行<text style="visibility:hidden">星级</text>业: <span>{{position.industry}}</span>
                </li>
                <li>
                    性<text style="visibility:hidden">星级</text>质: <span>{{position.xingzhi}}</span>
                </li>
                <li>
                    规<text style="visibility:hidden">星级</text>模: <span>{{position.guimo}}</span>
                </li>
                <li>
                    星<text style="visibility:hidden">星级</text>级: 
                    <span wx:for="{{position.xingji}}">
                        <image class="image3" src="image/_09.png"></image>
                    </span>
                </li>
                <li class="zhishu">
                    雇主指数: <span>{{position.zhishu}}</span>
                </li>
            </ul>
        </view>
        <view class="line"></view>
        <view class="biaoqian">
            <view wx:for="{{biaoqian}}">{{item}}</view>
        </view>
        <!--企业简介-->
        <view class="p_d_detail">企业简介</view>
        <view class="company_industry">
            默认情况下，弹性盒容器中的条目会尽量占满容器在主轴方向上的一行。当容器的主轴尺寸小于其所有条目总的主轴尺寸时，会出现条目之间互相重叠或超出容器范围的现象。CSS 属性"flex-wrap"用来声明当容器中条目的尺寸超过主轴尺寸时应采取的行为。"flex-wrap"属性的可选值及其
        </view>
        <!--联系我们-->
        <view class="p_d_detail">联系我们</view>
        <view class="contant_ours">
            <view>
                <span>联 系 人: </span>{{position.linkman}}
            </view>
            <view class="call">
                <span>联系电话: </span>{{position.phone}}
                <image data-number="{{position.phone}}" bindtap="call" src="image/phone.png" ></image>
            </view>
            <view>
                <span>邮<a style="visibility:hidden;">地地</a>箱: </span>{{position.email}}
            </view>
            <view>
                <span>地<a style="visibility:hidden;">地地</a>址: </span>{{position.address}}
                <image data-latitude="{{latitude}}" data-longitude="{{longitude}}" bindtap="site" src="image/site.png" ></image>
            </view>
        </view>
        <!--该企业其他职位-->
        <view class="p_d_detail">该企业其他职位</view>
        <view class="remenList">
            <block wx:for="{{otherList}}">
                <navigator redirect="true" url="../jobDetail/jobDetail">
                        <view class="list" id="{{item.id}}">
                            <view class="left">
                                <view class="positionName">{{item.positionName}}</view>
                                <view class="time">{{item.time}}</view>
                            </view>
                            <view class="right">
                                <view>{{item.money}}</view>
                                <view>{{item.action}}</view>
                            </view>
                        </view>
                </navigator>
            </block>
        </view>
        <button disabled="" loading="" class="guanzhu"> <image class="image3" src="image/_10.png"></image> 加关注</button>
</view>
